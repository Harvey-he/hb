#!/usr/bin/env node

var argv = require('optimist').argv;
var program = require('commander');
var log = console.log;

program
  .version(require('../package').version, '-v, --version')
  .usage('<command> [options]')
  .on('--help', printHelp)
  .parse(process.argv);

if(!argv[0]){
  program.help();
}

function printHelp() {
  log([
    "  Commands:",
    "",
    "    init           generate inital project",
    ""
  ].join('\n'));
  process.exit();
}
